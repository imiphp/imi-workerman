{
    "name": "imiphp/imi-workerman",
    "type": "library",
    "license": "MulanPSL-2.0",
    "description": "imi workerman component",
    "require": {
        "workerman/workerman": "^4.0",
        "workerman/channel": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.6",
        "wrench/wrench": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Imi\\Workerman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imi\\Workerman\\Test\\": "tests/unit/"
        },
        "files": [
            "../../../vendor/autoload.php"
        ]
    },
    "bin": [
        "bin/imi-workerman"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}